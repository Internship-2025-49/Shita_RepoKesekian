generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Person {
  id        Int      @id @default(autoincrement())
  name     String   @db.VarChar(255)
  address   String @db.VarChar(255)
  phone String @db.VarChar(255)

  @@map("person")
}

model Auth {
  key       String   @id 

